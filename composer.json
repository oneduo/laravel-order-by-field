{
    "name": "oneduo/laravel-order-by-field",
    "description": "Laravel query builder order by field method",
    "keywords": [
        "oneduo",
        "laravel",
        "laravel-order-by-field"
    ],
    "homepage": "https://github.com/oneduo/laravel-order-by-field",
    "license": "MIT",
    "authors": [
        {
            "name": "MikaÃ«l Popowicz",
            "email": "mikael@popowicz.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0 || ^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.6",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Oneduo\\LaravelOrderByField\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oneduo\\LaravelOrderByField\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oneduo\\LaravelOrderByField\\LaravelOrderByFieldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
